@using CarDealership.Models;
<h1>Best Car Dealership Ever</h1>

@if (Model.Count == 0)
{
  <p>There are no cars listed.</p>
}

  @foreach (Car car in Model)
  {
    <h5>@car.MakeModel</h5>
    <ul>
    <li>Price: @car.Price</li>
    <li>Miles: @car.Miles</li>
    </ul>
    
  }


<a href="/Cars/new">Add a car.</a>
<a href='/Cars'>See list of cars</a>
